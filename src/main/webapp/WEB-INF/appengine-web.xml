<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>jamfufu</application>
    <version>2</version>

    <system-properties>
        <property name="in.gae.j" value="true"/>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/logging.properties"/>
        <property name="com.facebook.app_id" value="${com.facebook.app_id}" />
        <property name="com.facebook.api_key" value="${com.facebook.api_key}" />
        <property name="com.facebook.secret" value="${com.facebook.secret}" />
        <property name="app.url" value="${app.url}" />
    </system-properties>

    <sessions-enabled>true</sessions-enabled>

    <static-files>
        <exclude path="/**"/>
    </static-files>

</appengine-web-app>
